{{#if products}}
    <h1>Lista de productos</h1>
{{else}}
    <h1>No hay productos en este momento</h1>
{{/if}}

{{#each products}}
    <div>
        <p>{{this.title}}</p>
        <p>{{this.description}}</p>
        <p>{{this.code}}</p>
        <p>{{this.price}}</p>
        <p>{{this.status}}</p>
        <p>{{this.stock}}</p>
        <p>{{this.category}}</p>
        <p>{{this.thumbnails}}</p>
    </div>
    <br/>
{{/each}}